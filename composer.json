{
	"name": "leuchtfeuer/mautic-lehner-site",
	"description": "Project setup for the Lehner Mautic installation including customizations, plugins and themes.",
	"license": "proprietary",
	"type": "project",
	"authors": [
		{
			"name": "Leuchtfeuer",
			"email": "dev@leuchtfeuer.com"
		}
	],
	"config": {
		"allow-plugins": {
			"composer/installers": true,
			"symfony/flex": true,
			"mautic/core-composer-scaffold": true,
			"mautic/core-project-message": true,
			"php-http/discovery": true,
			"cweagans/composer-patches": true
		},
		"sort-packages": true
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"repositories": [
		{
			"type": "git",
			"url": "https://github.com/mautic/FOSOAuthServerBundle.git"
		}, {
			"type": "vcs",
			"url": "git@github.com:Leuchtfeuer/mautic-leh-BoxalinoHistoryFeed.git"
		}, {
			"type": "vcs",
			"url": "git@github.com:TornMarketing/Mautic-Custom-Navlinks.git"
		}, {
			"type": "vcs",
			"url": "git@github.com:Leuchtfeuer/auth0-for-mautic.git"
		}, {
			"type": "vcs",
			"url": "git@github.com:Leuchtfeuer/mautic-bounce-monitor.git"
		}, {
			"type": "vcs",
			"url": "git@github.com:Leuchtfeuer/mautic-lehner-customerid-md5.git"
		}, {
			"type": "vcs",
			"url": "git@github.com:Leuchtfeuer/mautic-custom-url-parameters.git"
		}, {
			"type": "vcs",
			"url": "git@github.com:Leuchtfeuer/mautic-housekeeping-bundle.git"
		}, {
			"type": "vcs",
			"url": "git@github.com:Leuchtfeuer/mautic-lehner-csv-importer.git"
		}, {
			"type": "vcs",
			"url": "git@github.com:Leuchtfeuer/mautic-lehner-sovendus-api.git"
		}, {
			"type": "vcs",
			"url": "git@github.com:Leuchtfeuer/mautic-multiselect-handling-bundle.git"
		}, {
			"type": "path",
			"url": "./packages/*",
			"symlink": true
		}
	],
	"require": {
		"composer/installers": "^1.11",
		"cweagans/composer-patches": "^1",
		"diele/bounce-monitor": "^1.1",
		"lehner/mautic-plugin-customization": "dev-main",
		"lehner/mautic-scaffold": "dev-main",
		"leuchtfeuer/mautic-auth0-bundle": "^1.1",
		"leuchtfeuer/mautic-custom-url-parameters": "^1.0",
		"leuchtfeuer/mautic-customer-id-md5": "^1.0",
		"leuchtfeuer/mautic-housekeeping-bundle": "*",
		"leuchtfeuer/mautic-leh-boxalino-history-feed": "^1.0",
		"leuchtfeuer/mautic-lehner-csv-importer": "^1.0",
		"leuchtfeuer/mautic-lehner-sovendus-api": "^1.0",
		"leuchtfeuer/mautic-multiselect-handling": "^1.2",
		"mautic/core-composer-scaffold": "4.x-dev",
		"mautic/core-lib": "^4.4",
		"mautic/core-project-message": "4.x-dev",
		"mautic/grapes-js-builder-bundle": "^4.4",
		"mautic/plugin-citrix": "^4.4",
		"mautic/plugin-clearbit": "^4.4",
		"mautic/plugin-cloudstorage": "^4.4",
		"mautic/plugin-crm": "^4.4",
		"mautic/plugin-customnavigationlinksbundle": "dev-main",
		"mautic/plugin-emailmarketing": "^4.4",
		"mautic/plugin-focus": "^4.4",
		"mautic/plugin-fullcontact": "^4.4",
		"mautic/plugin-gmail": "^4.4",
		"mautic/plugin-outlook": "^4.4",
		"mautic/plugin-social": "^4.4",
		"mautic/plugin-tagmanager": "^4.4",
		"mautic/plugin-zapier": "^4.4",
		"mautic/theme-aurora": "^4.4",
		"mautic/theme-blank": "^4.4",
		"mautic/theme-brienz": "^4.4",
		"mautic/theme-cards": "^4.4",
		"mautic/theme-coffee": "^4.4",
		"mautic/theme-confirmme": "^4.4",
		"mautic/theme-fresh-center": "^4.4",
		"mautic/theme-fresh-fixed": "^4.4",
		"mautic/theme-fresh-left": "^4.4",
		"mautic/theme-fresh-wide": "^4.4",
		"mautic/theme-goldstar": "^4.4",
		"mautic/theme-mauve": "^4.4",
		"mautic/theme-nature": "^4.4",
		"mautic/theme-neopolitan": "^4.4",
		"mautic/theme-oxygen": "^4.4",
		"mautic/theme-paprika": "^4.4",
		"mautic/theme-skyline": "^4.4",
		"mautic/theme-sparse": "^4.4",
		"mautic/theme-sunday": "^4.4",
		"mautic/theme-trulypersonal": "^4.4",
		"mautic/theme-vibrant": "^4.4"
	},
	"require-dev": {
		"deployer/deployer": "^7.3",
		"friendsofphp/php-cs-fixer": "^2.19.2",
		"http-interop/http-factory-guzzle": "^1.0",
		"liip/functional-test-bundle": "^4.2",
		"liip/test-fixtures-bundle": "^1.6",
		"mautic/transifex": "^5.0",
		"php-http/mock-client": "^1.4",
		"phpunit/phpunit": "^9.5",
		"symfony/browser-kit": "~4.4.0",
		"symfony/dom-crawler": "~4.4.0",
		"symfony/phpunit-bridge": "~5.1.0",
		"symfony/var-dumper": "~4.4.0",
		"symfony/web-profiler-bundle": "~4.4.0",
		"webfactory/exceptions-bundle": "~4.3"
	},
	"conflict": {
		"mautic/mautic": "*",
		"mautic/core": "*"
	},
	"autoload": {
		"psr-4": {
			"MauticPlugin\\": "docroot/plugins/"
		}
	},
	"extra": {
		"mautic-scaffold": {
			"locations": {
				"web-root": "docroot/"
			},
			"allowed-packages": [
				"lehner/mautic-scaffold"
			]
		},
		"installer-paths": {
			"docroot/app": [
				"type:mautic-core"
			],
			"docroot/plugins/{$name}": [
				"type:mautic-plugin"
			],
			"docroot/themes/{$name}": [
				"type:mautic-theme"
			]
		},
		"patches-file": "patches/composer.patches.json"
	},
	"scripts": {
		"fix": [
			"@fix:composer",
			"@fix:editorconfig",
			"@fix:php"
		],
		"fix:composer": "@composer normalize",
		"fix:editorconfig": "ec --fix",
		"fix:php": "php-cs-fixer fix",
		"lint": [
			"@lint:composer",
			"@lint:editorconfig",
			"@lint:php"
		],
		"lint:composer": "@composer normalize --dry-run",
		"lint:editorconfig": "ec",
		"lint:php": "php-cs-fixer fix --dry-run",
		"test": [
			"@test:unit"
		],
		"test:unit": "phpunit -c phpunit.xml"
	},
	"scripts-descriptions": {
		"fix": "Fix all code quality issues reported by the registered linters",
		"fix:composer": "Fix all issues in `composer.json`",
		"fix:editorconfig": "Fix all styling issues violating the `.editorconfig` rules",
		"fix:php": "Fix all styling issues violating the configured PHP-CS-Fixer rules",
		"lint": "Run all registered linters to detect code quality issues",
		"lint:composer": "Lint `composer.json` to detect unnormalized styles",
		"lint:editorconfig": "Lint all project files to detect violations of the `.editorconfig` rules",
		"lint:php": "Lint all PHP files to detect coding style violations",
		"test": "Run all configured test suites",
		"test:unit": "Run unit tests for all local packages"
	}
}
